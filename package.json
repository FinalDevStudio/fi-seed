{
  "name": "fi-seed",
  "title": "Fi Seed",
  "description": "A boilerplate for [Node|io].js Express apps with AngularJS on the frontend",
  "version": "1.0.0",
  "private": false,
  "license": "MIT",
  "author": "Final Development Studio <sorcery@finaldevstudio.com> (http://finaldevstudio.com)",
