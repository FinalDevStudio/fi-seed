    "express-session": "^1.11.2",
    "gridfs-stream": "^1.1.1",
    "inflection": "^1.7.1",
    "jade": "^1.10.0",
    "lusca": "^1.1.0",
    "lwip": "0.0.7",
    "mkdirp": "^0.5.1",
    "mongoose": "^4.0.2",
    "morgan": "^1.6.0",
